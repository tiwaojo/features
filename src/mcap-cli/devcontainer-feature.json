{
    "name": "MCAP (via GitHub Releases)",
    "id": "mcap-cli",
    "version": "1.0.2",
    "description": "Install MCAP-cli useful for working with MCAP files",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "v0.0.46"
            ],
            "default": "latest",
            "description": "Select or enter your own MCAP-cli version"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "promethus.mcap-cli-vscode"
            ]
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
